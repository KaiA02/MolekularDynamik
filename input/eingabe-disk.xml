<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../src/inputReader/simulation.xsd">
    <input>
        <tStart>0.0</tStart>
        <tEnd>3.0</tEnd>
        <deltaT>0.0005</deltaT>
        <inputType>LJF</inputType>
        <particleContainerType>LC</particleContainerType>
        <r_cutoff>2.0</r_cutoff>
        <domainSizeX>25</domainSizeX>
        <domainSizeY>25</domainSizeY>
        <domainSizeZ>25</domainSizeZ>
        <boundary1Type></boundary1Type>
        <boundary2Type></boundary2Type>
        <boundary3Type></boundary3Type>
        <boundary4Type></boundary4Type>
        <boundary5Type></boundary5Type>
        <boundary6Type></boundary6Type>
        <particles>
            <x>10.0</x>
            <y>10.0</y>
            <z>10.0</z>
            <velocityX>-10.0</velocityX>
            <velocityY>0.0</velocityY>
            <velocityZ>0.0</velocityZ>
            <mass>1</mass>
        </particles>
        <disk>
            <radius>5</radius>
            <distance>1.1225</distance>
            <dimension>3</dimension>
        </disk>
    </input>
    <output>
        <baseName>simulation_output</baseName>
        <writeFrequency>100</writeFrequency>
        <outputType>vtk</outputType>
    </output>
    <config>
        <performanceMeasurement>false</performanceMeasurement>
        <logLevel>warn</logLevel>
    </config>
</simulation>
